<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <ul>
      <% messages.forEach(function(message) { %>
        <li>
          <p><%= message.user %></p>
          <p><%= message.text %></p>
          <p><%= message.added %></p>
        </li>
      <% }) %>
    </ul>
    
  </body>
</html>
